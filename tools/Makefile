all: ppm2raw raw2rle rle2raw wav2pcm pcm2clw clw2pcm

ppm2raw: ppm2raw.c
	gcc -o ppm2raw ppm2raw.c

raw2rle: raw2rle.c
	gcc -o raw2rle raw2rle.c

rle2raw: rle2raw.c
	gcc -o rle2raw rle2raw.c

wav2pcm: wav2pcm.c
	gcc -o wav2pcm wav2pcm.c

pcm2clw: pcm2clw.c
	gcc -o pcm2clw pcm2clw.c

clw2pcm: clw2pcm.c
	gcc -o clw2pcm clw2pcm.c
